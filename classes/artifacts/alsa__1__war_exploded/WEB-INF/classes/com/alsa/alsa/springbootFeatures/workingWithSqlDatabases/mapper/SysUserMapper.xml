<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alsa.alsa.springbootFeatures.workingWithSqlDatabases.mapper.SysUserMapper">
	<select id="selectUser" resultType="com.alsa.alsa.springbootFeatures.workingWithSqlDatabases.dto.User">
        select user_id userId,user_name userName from sys_user where user_name = #{userName}
    </select>
</mapper>